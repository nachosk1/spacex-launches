---
import Layout from "../layouts/Layout.astro";

import Launches from '../components/Launches.astro';
---

<Layout title="Lo más viejos Lanzamientos">
  <div class="max-w-7xl mx-auto mt-4 md:mt-10 px-4">
    <h1
      class="mb-4 text-4xl tracking-tight font-extrabold text-white flex gap-8 items-center"
    >
      Los Lanzamientos más antiguos
    </h1>
    <Launches shortBy="desc" />
  </div>
</Layout>
